<div>
   <%= link_to "Logout", "/" %>
</div>
<h1>Listing Brands</h1> 
<table>
  <tr>
    <th>Name</th>
    <th>Slogan</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% @brands.each do |brand| %>
  <tr>
    <td><%= brand.name %></td>
    <td><%= brand.slogan %></td>
    <td><%= link_to 'Show', brand %></td>
    <td><%= link_to 'Edit', edit_brand_path(brand) %></td>
    <td><%= link_to 'Delete', brand, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<br />
<%= link_to "New brand", new_brand_path %>

